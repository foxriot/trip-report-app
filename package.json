{
  "name": "TripReport",
  "description": "Trip Report",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "author": {
    "email": "an@whatsthis.one",
    "name": "Ã…n",
    "url": "https://intrepidpixel.org/"
  },
  "engines": {
    "node": ">18.12"
  },
  "main": "packages/main/dist/index.cjs",
  "scripts": {
    "build": "npm run build:main && npm run build:preload && npm run build:renderer",
    "build:main": "cd ./packages/main && vite build",
    "build:preload": "cd ./packages/preload && vite build",
    "build:renderer": "cd ./packages/renderer && vite build",
    "compile": "cross-env MODE=production npm run build && electron-builder build --config .electron-builder.config.cjs --dir --config.asar=false",
    "compile-asar": "cross-env MODE=production npm run build && electron-builder build --config .electron-builder.config.cjs --dir --config.asar=true --config.asarUnpack packages/main/dist/standalone**/**",
    "test": "npm run test:main && npm run test:preload && npm run test:renderer && npm run test:e2e",
    "test:e2e": "npm run build && vitest run",
    "test:main": "vitest run -r packages/main --passWithNoTests",
    "test:preload": "vitest run -r packages/preload --passWithNoTests",
    "test:renderer": "vitest run -r packages/renderer --passWithNoTests",
    "watch": "node scripts/watch.mjs",
    "watchMac": "node scripts/watch.mjs 2>&1| grep -v 'CoreText note:'",
    "lint": "eslint . --ext js,mjs,cjs,ts,mts,cts,vue",
    "typecheck:main": "tsc --noEmit -p packages/main/tsconfig.json",
    "typecheck:preload": "tsc --noEmit -p packages/preload/tsconfig.json",
    "typecheck:renderer": "tsc --noEmit -p packages/renderer/tsconfig.json",
    "typecheck": "npm run typecheck:main && npm run typecheck:preload && npm run typecheck:renderer",
    "postinstall": "cross-env ELECTRON_RUN_AS_NODE=1 electron scripts/update-electron-vendors.mjs",
    "format": "npx prettier --write \"**/*.{js,mjs,cjs,ts,mts,cts,vue,json}\""
  },
  "devDependencies": {
    "@types/node": "18.11.18",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@types/sharp": "^0.31.1",
    "@types/tmp": "^0.2.3",
    "@typescript-eslint/eslint-plugin": "5.48.1",
    "cross-env": "7.0.3",
    "electron": "24.1.2",
    "electron-builder": "23.6.0",
    "eslint": "8.31.0",
    "eslint-plugin-vue": "9.9.0",
    "happy-dom": "8.1.4",
    "nano-staged": "0.8.0",
    "playwright": "1.29.2",
    "typescript": "4.9.4",
    "unplugin-auto-expose": "0.0.4",
    "vite": "4.0.4",
    "vite-plugin-static-copy": "^0.13.1",
    "vitest": "0.27.1"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.1",
    "@vitejs/plugin-react": "^3.0.1",
    "adm-zip": "^0.5.10",
    "asar-node": "^3.1.1",
    "chokidar": "^3.5.3",
    "electron-updater": "5.3.0",
    "exiftool-vendored": "^19.0.0",
    "false": "^0.0.4",
    "fdir": "^5.3.0",
    "find-config": "^1.0.0",
    "humanize-duration": "^3.28.0",
    "image-size": "^1.0.2",
    "image-thumbnail": "^1.0.15",
    "JSONStream": "^1.3.5",
    "jszip": "^3.10.1",
    "jszip-utils": "^0.1.0",
    "knex": "^2.4.2",
    "luxon": "^3.2.1",
    "mousetrap": "^1.6.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.8.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.7.0",
    "sharp": "^0.32.0",
    "spinners-react": "^1.0.7",
    "sqlite3": "^5.1.4",
    "uuid": "^9.0.0",
    "wql-process-monitor": "^3.0.0"
  }
}
